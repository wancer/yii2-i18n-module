{
  "name": "wancer/yii2-i18n-module",
  "based-on": "zelenin/yii2-i18n-module",
  "description": "Yii2 i18n (internalization) module makes the translation of your application so simple",
  "version": "0.2.0",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "i18n",
    "translation",
    "translations"
  ],
  "homepage": "https://github.com/zelenin/yii2-i18n-module",
  "time": "2015-09-16",
  "license": "MIT",
  "authors": [
    {
      "name": "Aleksandr Zelenin",
      "email": "aleksandr@zelenin.me",
      "homepage": "http://zelenin.me",
      "role": "Developer"
    },
    {
      "name": "Wancer"
    }
  ],
  "require": {
    "yiisoft/yii2": "~2"
  },
  "autoload": {
    "psr-4": {
      "Wancer\\yii\\modules\\I18n\\": ""
    }
  },
  "extra": {
    "bootstrap": "Wancer\\yii\\modules\\I18n\\Bootstrap"
  }
}
